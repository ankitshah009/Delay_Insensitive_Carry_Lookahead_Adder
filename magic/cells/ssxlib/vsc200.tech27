	squares 200
	calma 25 1
    layer CCC polyct/m1,poly12/m1
	squares 200
	calma 25 1
    layer CCC gc
	calma 25 1
    layer CV1 m2c/m1
	squares 200
	calma 50 1
    layer CM1 m1,pdifct/m1,ndifct/m1,ptiect/m1,ntiect/m1,polyct/m1,m2c/m1
	labels m1,pdifct,ndifct,ptiect,ntiect,polyct,m2c
	calma 49 1
    layer CV2 m3c/m2
	squares 200
	calma 61 1
    layer CM2 m2,m2c/m2,m3c/m2
	labels m2,m2c,m3c
	calma 51 1
    layer CV3 m4c/m3
	squares 200
	calma 30 1
