cifinput
style lambda=1um(vsclib200)
    scalefactor 100
    layer ab AB
        labels AB 
    layer nwell CWN
	labels CWN
    layer pwell CWP
	labels CWP
    layer poly CPG
        labels CPG
    layer pdiff CPD
	labels CPD
    layer ndiff CND
	labels NPD
    layer ntie CNS
    layer ptie CPS
    layer nfet CPG
        and CAA
        and CSN
    layer pfet CAA
        and CPG
        and CSP
    layer ndifct CCC
        grow 100
        and CM1
        grow 100
	and CWP
        and CAA
        and CSN
        shrink 100
        calma CCC 25 *
    layer pdifct CCC
        grow 100
        and CM1
        grow 100
        and CWN
        and CAA
        and CSP
        shrink 100
        calma CCC 25 *
    layer ntiect CCC
        grow 100
        and CM1
        grow 100
        and CWN
        and CAA
        and CSN
        shrink 100
        calma CCC 25 *
    layer ptiect CCC
        grow 100
        and CM1
        grow 100
        and CWP 
        and CAA
        and CSP
        shrink 100
        calma CCC 25 *
    layer polyct CCC
        grow 100
        and CM1
        grow 100
        and CPG
        shrink 100
        calma CCC 25 *
    layer m1 CM1
        grow 100
        shrink 100
        labels CM1
	calma CM1 49 *
    layer m2c CV1
        grow 100
        and CM2
        and CM1
        labels CM2
        calma CV1 50 *
    layer m2 CM2
        grow 100
        shrink 100
        labels CM2
	calma CM2 51 *
    layer m3c CV2
        grow 100
        and CM3
        and CM2
        labels CM3
        calma CV2 61 *
    layer m3 CM3
        grow 100
        shrink 100
