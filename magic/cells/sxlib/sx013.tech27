cifoutput
style lambda=0.055um(sxlib013)
    scalefactor 55 1 nanometers
    layer AB ab
        labels ab
        calma 63 1
    templayer XWN nwell
    templayer XWP pwell
    layer CWN nwell
        and AB
	grow 220
	and-not XWP
	shrink 110
	grow 110
	calma 42 1
    layer CWP pwell
        and AB
	grow 220
        and-not XWN
	shrink 110
	grow 110
	calma 41 1
    layer CPD pdiff,pdifct/a,pfet
	shrink 5
	labels pdiff
	calma 4 1
    layer CND ndiff,ndifct/a,nfet
	shrink 5
	labels ndiff
	calma 3 1
    layer CNS ntie,ntiect/a
	shrink 5
	calma 5 1
    layer CPS ptie,ptiect/a
	shrink 5
	calma 6 1
    layer CAA ndiff,ndifct/a,nfet,pdiff,pdifct/a,pfet,ntie,ntiect/a,ptie,ptiect/a
	shrink 5
	labels ndiff,pdiff
	calma 43 1
    layer CPG poly,polyct/a,nfet,pfet
        grow 5
	labels poly,nfet,pfet
	calma 46 1
    layer CCC ndifct/m1,pdifct/m1
	squares 160
	calma 25 1
    layer CCC ntiect/m1,ptiect/m1
	squares 160
	calma 25 1
    layer CCC polyct/m1
	squares 160
	calma 25 1
    layer CCC gc
	calma 25 1
    layer CV1 m2c/m1
	squares 200
	calma 50 1
    templayer XCM1 ndifct/m1,pdifct/m1,ntiect/m1,ptiect/m1,polyct/m1,m2c/m1
	grow 50
    layer CM1 m1,ndifct/m1,pdifct/m1,ntiect/m1,ptiect/m1,polyct/m1,m2c/m1
	shrink 5
	or XCM1
	and AB
	labels m1
	calma 49 1
    layer CM2 m2,m2c/m2,m3c/m2
	labels m2,m2c,m3c
	calma 51 1
    templayer XTN ntie,ntiect/a
	grow 40
    templayer XTP ptie,ptiect/a
	grow 40
    layer CSN pwell
        and AB
        grow 125
        and CWP
	and-not XTP
	calma 45 1
    layer CSN ntie,ntiect/a
	grow 40
	calma 45 1
    layer CSP nwell
        and AB
        grow 125
        and CWN
	and-not XTN
	calma 44 1
    layer CSP ptie,ptiect/a
	grow 40
	calma 44 1
    layer COG pad
	shrink 330
	or glass
	or open
	labels pad
	calma 52 1
    layer XP pad
	shrink 330
	calma 26 1
end

